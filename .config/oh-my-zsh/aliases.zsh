alias cob="git checkout -b "
alias com="git checkout main"
alias gap="git add -p"
alias gc="killall gpg-agent && gpg-agent --daemon --pinentry-program /usr/local/bin/pinentry && git commit -v"
alias gp="git pull"
alias gps="git push -u origin head"
alias gpsf="git push -u origin head --no-verify --force"
alias gs="git status"
alias gundo="git reset HEAD~"
alias n=fnm
alias upgrade="zsh ~/.config/upgrade.zsh"
alias us="git reset HEAD ."
